 "html": "{%- macro add_header(page_num, max_pages, doc, letter_head, no_letterhead, footer, print_settings=None) -%}\n\t{% if letter_head and not no_letterhead %}\n\t\t<div class=\"letter-head\" style=\"padding-top: 20px; margin-bottom: -5mm !important\">{{ letter_head }}</div>\n\t{% endif %}\n\t{%- if doc.meta.is_submittable and doc.docstatus==0 and (print_settings==None or print_settings.add_draft_heading) -%}\n\t<div class=\"text-center\" document-status=\"draft\">\n\t\t<h4 style=\"margin: -20px;\">{{ _(\"DRAFT\") }}</h4>\n\t</div>\n\t{%- endif -%}\n\t{%- if doc.meta.is_submittable and doc.docstatus==2-%}\n\t<div class=\"text-center\" document-status=\"cancelled\">\n\t\t<h4 style=\"margin: 0px;\">{{ _(\"CANCELLED\") }}</h4>\n\t</div>\n\t{%- endif -%}\n{%- endmacro -%}\n\n{% for page in layout %}\n<div class=\"page-break\">\n\t<div {% if print_settings.repeat_header_footer %} id=\"header-html\" class=\"hidden-pdf\" {% endif %}>\n\t\t{{ add_header(loop.index, layout|len, doc, letter_head, no_letterhead, footer, print_settings) }}\n\t</div>\n\n\t{% if print_settings.repeat_header_footer %}\n\t<div id=\"footer-html\" class=\"visible-pdf\">\n\t\t{% if not no_letterhead and footer %}\n\t\t<div class=\"letter-head-footer\">\n\t\t\t{{ footer }}\n\t\t</div>\n\t\t{% endif %}\n\t\t<p class=\"text-center small page-number visible-pdf\">\n\t\t\t{{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\n\t\t</p>\n\t</div>\n\t{% endif %}\n\t <table class=\"table table-condensed table-bordered\" style=\"border:1px solid black;width:100%; margin-top:15px;\">\n        <tr>\n            <td  style=\"width:16%;border:1px solid black;\"><b>Name:</b></td>\n            <td style=\"width:34%;border:1px solid black;\">{{ doc.employee_name }}</td>\n            <td style=\"width:16%;border:1px solid black;\"><b>Employee No:</b></td>\n            <td style=\"width:34%;border:1px solid black;\">{{ doc.employee }}</td>\n        </tr>\n        <tr>\n            <td  style=\"border:1px solid black;\"><b>Joining Date:</b>:</td>\n            <td  style=\"border:1px solid black;\">\n                {% if (frappe.db.get_value(\"Employee\", doc.employee, \"date_of_joining\")) %}\n                {{ frappe.format(frappe.db.get_value(\"Employee\",doc.employee, \"date_of_joining\"), {'fieldtype': 'Date'}) }}\n                {% endif %}</td>\n            <td  style=\"border:1px solid black;\"><b>Posting Date</b></td>\n            <td  style=\"border:1px solid black;\">{{ frappe.format(doc.posting_date, {'fieldtype': 'Date'}) }}</td>\n        </tr>\n        <tr>\n           <td  style=\"border:1px solid black;\"><b>Designation:</b></td>\n            <td  style=\"border:1px solid black;\">{{doc.designation}}</td>\n            <td  style=\"border:1px solid black;\"><b>Department:</b></td>\n            <td  style=\"border:1px solid black;\">{{doc.department}}</td>\n        </tr>\n        <tr>\n            <td  style=\"border:1px solid black;\"><b>Bank Account:</b></td>\n            <td  style=\"border:1px solid black;\">{{ doc.bank_name or \"\" }}</td>\n            <td   style=\"border:1px solid black;\" rowspan=\"\"> <b><nobr>Bank Account No:</nobr></b></td>\n            <td  style=\"border:1px solid black;\"  rowspan=\"\">{{ doc.bank_account_no  or \"\"}}</td>\n        </tr>\n        <tr>\n            <td  style=\"border:1px solid black;\"><b><nobr>Posting Month:</nobr></b></td>\n            <td  style=\"border:1px solid black;\">\n                {{ frappe.format_date(doc.start_date, \"MMMM\") }}</td>\n            <td  style=\"border:1px solid black;\"><b>Pan Number:</b></td>\n            <td  style=\"border:1px solid black;\">\n                {% if (frappe.db.get_value(\"Employee\", doc.employee, \"pan_number\")) %}\n                {{frappe.db.get_value(\"Employee\",doc.employee, \"pan_number\") or ''}} \n                {% endif %}\n            </td>\n        </tr>\n        <tr>\n            <td  style=\"border:1px solid black;\"><b>PF No.</b></td>\n            \n            <td  style=\"border:1px solid black;\">\n                {% if (frappe.db.get_value(\"Employee\", doc.employee, \"provident_fund_account\")) %}\n                {{frappe.db.get_value(\"Employee\",doc.employee, \"provident_fund_account\") or ''}} \n                {% endif %}</td>\n            <td  style=\"border:1px solid black;\"><b>PF UAN:</b></td>\n            <td  style=\"border:1px solid black;\">\n                {% if (frappe.db.get_value(\"Employee\", doc.employee, \"uan_number\")) %}\n                {{frappe.db.get_value(\"Employee\",doc.employee, \"uan_number\") or ''}} \n                {% endif %}\n            </td>\n        </tr>\n         <tr>\n            <td  style=\"border:1px solid black;\"><b><nobr>Effective Work Days:</nobr></b></td>\n            <td  style=\"border:1px solid black;\">{{ doc.payment_days }}</td>\n            <td  style=\"border:1px solid black;\"><b>LOP:</b></td>\n            <td  style=\"border:1px solid black;\"><b>{{ doc.leave_without_pay|int or \"\" }}</b></td>\n        </tr>\n        <tr>\n            <td  style=\"border:1px solid black;\"><b>ESI No</b></td>\n            <td  style=\"border:1px solid black;\">\n                {% if (frappe.db.get_value(\"Employee\", doc.employee, \"esi_number\")) %}\n                {{frappe.db.get_value(\"Employee\",doc.employee, \"esi_number\") or ''}} \n                {% endif %}\n            </td>\n            <td  style=\"border:1px solid black;\"><b>Absent Days</b></b></td>\n            <td  style=\"border:1px solid black;\">{{ doc.absent_days }}</td>\n            <!--<td  style=\"border:1px solid black; border-right:1px solid white;\"></td>-->\n            <!--<td  style=\"border:1px solid black;\"></td>-->\n        </tr>\n        \n        </table>\n        <div class=\"row\">\n        <div class=\"col-xs-6\" style=\"margin-top:-5%;\">\n        <table class=\"table table-condensed table-bordered\" style=\"border:1px solid black; width:100%;\">\n            <tr>\n                <td style=\"border:1px solid black;\"><b>Earnings</b></td>\n                <td style=\"border:1px solid black;\"><b>Amount</b></td>\n            </tr>\n            {% for row in doc.earnings %}\n            {% if row.salary_component != \"ESI - Employer(N)\" and row.salary_component != \"EPF - Employer(N)\" %}\n            <tr>\n                <td style=\"border:1px solid black;\">{{ row.salary_component }}</td>\n                <td style=\"border:1px solid black;\">{{ row.amount }}</td>\n            </tr>\n             \n            {% endif %}\n            {% endfor %}\n            <tr style=\"border:1px solid black !important;\">\n                <td style=\"border:1px solid black;\"><b>Total Earnings:INR.</b></td>\n                <td style=\"border:1px solid black !important;\"><b>{{ doc.gross_pay }}</b></td>\n            </tr>\n        </table>\n    </div>\n    <div class=\"col-xs-6\" style=\"margin-left:0%; margin-top:-5%;\">\n        <table class=\"table table-condensed table-bordered\" style=\"border:1px solid black;width:100%;\">\n            <tr>\n                <td style=\"border:1px solid black !important;\"><b>Deductions</b></td>\n                <td style=\"border:1px solid black !important;\"><b>Amount</b></td>\n            </tr>\n            {% for i in doc.deductions %}\n            {% if  i.salary_component in [\"ESI-Employee\",\"PF-Employee\",\"ESI-Employee(N)\",\"PF-Employee(N)\",\"Late Coming -deduction\",\"Salary Advance\",\"Professional Tax\"] %}\n             <tr>\n                <td style=\"border:1px solid black !important;\">{{ i.salary_component }}</td>\n                <td style=\"border:1px solid black !important;\">{{i.amount }}</td>\n            </tr>\n            {% endif %}\n            \n            {% endfor %}\n            <tr style=\"border:1px solid black !important;\">\n                <td style=\"border:1px solid black !important;\"><b>Total Deductions:INR.</b></td>\n                <td style=\"border:1px solid black !important;\"><b>{{ \"{:,.2f}\".format(doc.total_deduction|float) }}</b></td>\n            </tr>\n        </table>\n    </div>\n    </div>\n    <table style=\"width:100%; margin-top:-2%;\">\n        <tr style=\"border:1px solid black; border-left:1px solid black; border-right:1px solid black;\">\n            <td style=\"width:50%;\"><b>Net Pay for the month(Total Earnings - Total deductions):</b> {{ doc.net_pay }}<br>\n            ({{ doc.base_total_in_words }})\n            </td>\n        </tr>\n     </table>  \n</div>\n{% endfor %}\n<!--{%- macro add_header(page_num, max_pages, doc, letter_head, no_letterhead, footer, print_settings=None) -%}-->\n<!--\t{% if letter_head and not no_letterhead %}-->\n<!--\t\t<div class=\"letter-head\" style=\"padding-top: 20px; margin-bottom: -5mm !important\">{{ letter_head }}</div>-->\n<!--\t{% endif %}-->\n<!--\t{%- if doc.meta.is_submittable and doc.docstatus==0 and (print_settings==None or print_settings.add_draft_heading) -%}-->\n<!--\t<div class=\"text-center\" document-status=\"draft\">-->\n<!--\t\t<h4 style=\"margin: -20px;\">{{ _(\"DRAFT\") }}</h4>-->\n<!--\t</div>-->\n<!--\t{%- endif -%}-->\n<!--\t{%- if doc.meta.is_submittable and doc.docstatus==2-%}-->\n<!--\t<div class=\"text-center\" document-status=\"cancelled\">-->\n<!--\t\t<h4 style=\"margin: 0px;\">{{ _(\"CANCELLED\") }}</h4>-->\n<!--\t</div>-->\n<!--\t{%- endif -%}-->\n<!--{%- endmacro -%}-->\n\n<!--{% for page in layout %}-->\n<!--<div class=\"page-break\">-->\n<!--\t<div {% if print_settings.repeat_header_footer %} id=\"header-html\" class=\"hidden-pdf\" {% endif %}>-->\n<!--\t\t{{ add_header(loop.index, layout|len, doc, letter_head, no_letterhead, footer, print_settings) }}-->\n<!--\t</div>-->\n\n<!--\t{% if print_settings.repeat_header_footer %}-->\n<!--\t<div id=\"footer-html\" class=\"visible-pdf\">-->\n<!--\t\t{% if not no_letterhead and footer %}-->\n<!--\t\t<div class=\"letter-head-footer\">-->\n<!-